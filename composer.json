{
    "name": "artisanry/stripe",
    "description": "A Stripe bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Stripe",
        "Laravel Stripe"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "graham-campbell/manager": "^3.0",
        "illuminate/support": "^5.8|^6.0",
        "stripe/stripe-php": "^5.9"
    },
    "require-dev": {
        "graham-campbell/testbench": "5.3.1",
        "mockery/mockery": "1.2.4",
        "phpunit/phpunit": "6.5.14"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Stripe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Stripe\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Stripe\\StripeServiceProvider"
            ],
            "aliases": {
                "Stripe": "Artisanry\\Stripe\\Facades\\Stripe"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
